<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <PackageId>RSCCo.ConfigurationHelper</PackageId>
	  <Version>1.0.0</Version>	  
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <RepositoryUrl>https://github.com/MarshalOfficial/ConfigurationHelper</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>	  
	  <Description>Configuration Helper helps to get configuration values, first look Environment variables then appsettings.json or every file that has been loaded from IConfiguration instance.</Description>
	  <Company>RSC</Company>
	  <Authors>MarshalOfficial</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="6.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="6.0.0" />
  </ItemGroup>

</Project>
